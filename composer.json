{
    "name": "delickate/security",
    "description": "Laravel package to add security",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sani Hyne",
            "email": "delickate@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Delickate\\Security\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Delickate\\Security\\Providers\\UserSessionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
